// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Landlord Entity

model Lanlord {
  id      Int      @id @default(autoincrement())
  firstName   String?   
  lastName    String?
  mobileNo   String? @unique
  property Property[]
}








